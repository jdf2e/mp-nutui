<a style="background-color:{{bgColor}}" class="{{['nut-cell', isLink ? 'nut-cell-link' : '']}}" href="{{linkUrl||'javascript:;'}}">
  <view class="nut-cell-box " bind:tap="open" data-args0="{{e}}" data-args1="{{item}}" data-args2="{{true}}">
    <view class="nut-cell-left ">
      <text class="nut-cell-title ">
        <slot class="" name="title">
          {{title}}
        </slot>
      </text>
      <text class="nut-cell-sub-title ">
        <slot class="" name="sub-title">
          {{subTitle}}
        </slot>
      </text>
    </view>
    <view class="nut-cell-right ">
      <text class="nut-cell-desc ">
        <slot class="" name="desc">
          {{desc}}
        </slot>
      </text>
      <text class="nut-cell-icon ">
        <slot class="" name="icon" wx:if="{{showIcon}}">
          <image class="" src="data:image/svg+xml,%3Csvg viewBox='0 0 5 10' xmlns='http://www.w3.org/2000/svg'%3E%3Cpath d='M1.215 9.757l3.577-4.17a.931.931 0 0 0 0-1.173L1.215.244a.642.642 0 0 0-1.007 0 .929.929 0 0 0 0 1.172L3.283 5 .208 8.584a.93.93 0 0 0 0 1.173.643.643 0 0 0 1.007 0z' fill='%23c8c8cd'/%3E%3C/svg%3E" alt="">
          </image>
        </slot>
      </text>
    </view>
  </view>
</a>